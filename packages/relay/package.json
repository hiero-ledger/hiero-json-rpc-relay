{
  "name": "@hashgraph/json-rpc-relay",
  "version": "0.71.0-SNAPSHOT",
  "description": "Hedera Hashgraph implementation of Ethereum JSON RPC APIs. Utilises both the Hedera Consensus Nodes and the Mirror Nodes for transaction management and information retrieval",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Hedera Smart Contracts Team",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.14",
    "chai": "^4.3.6",
    "redis-memory-server": "^0.12.1",
    "sinon": "^14.0.0",
    "ts-mocha": "^9.0.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "lint": "npx eslint --ext .js,.ts . --ignore-path ../../.eslintignore",
    "format": "npx prettier --ignore-path ../../.gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' --exit",
    "test-eth": "c8 ts-mocha --recursive './tests/lib/eth/**/*.spec.ts' --exit",
    "test:eth-get-block-by-number": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetBlockByNumber' --exit",
    "test:eth-get-block-receipts": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetBlockReceipts' --exit",
    "test:eth-get-block-by-hash": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetBlockByHash' --exit",
    "test:eth-get-block-transaction-count-by-number": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetBlockTransactionCountByNumber' --exit",
    "test:eth-get-block-transaction-count-by-hash": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetBlockTransactionCountByHash' --exit",
    "test:eth-get-transaction-by-block-number-and-index": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetTransactionByBlockNumberAndIndex' --exit",
    "test:eth-get-transaction-by-block-hash-and-index": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetTransactionByBlockHashAndIndex' --exit",
    "test:eth-get-balance": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetBalance' --exit",
    "test:eth-get-code": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetCode' --exit",
    "test:eth-fee-history": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethFeeHistory' --exit",
    "test:eth-estimate-gas": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethEstimateGas' --exit",
    "test:eth-gas-price": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGasPrice' --exit",
    "test:eth-call": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethCall' --exit",
    "test:eth-send-raw-transaction": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethSendRawTransaction' --exit",
    "test:eth-get-storage-at": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetStorageAt' --exit",
    "test:eth-get-transaction-count": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetTransactionCount' --exit",
    "test:eth-common": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethCommon' --exit",
    "test:eth-get-transaction-receipt": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetTransactionReceipt' --exit",
    "test:eth-get-transaction-by-hash": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetTransactionByHash' --exit",
    "test:eth-get-logs": "c8 ts-mocha --recursive './tests/**/*.spec.ts' './tests/**/**/*.spec.ts' -g '@ethGetLogs' --exit"
  },
  "dependencies": {
    "@hashgraph/json-rpc-config-service": "file:../config-service",
    "@hashgraph/sdk": "^2.63.0",
    "axios": "^1.4.0",
    "axios-retry": "^4.5.0",
    "better-lookup": "^1.3.0",
    "dotenv": "^17.2.1",
    "ethers": "^5.8.0",
    "find-config": "^1.0.0",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.0",
    "pino": "^9.7.0",
    "redis": "^4.6.7"
  }
}
